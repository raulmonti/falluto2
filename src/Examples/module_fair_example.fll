OPTIONS

FLLNAME modulefairoutput

--MODULE_WEAK_FAIR_DISABLE

CHECK_DEADLOCK

ENDOPTIONS


MODULE mod1()
	VAR
		var1: bool
	TRANS
		[trans1]: var1 = TRUE => var1' = !var1
ENDMODULE

MODULE mod2()
	VAR
                var1: bool
        TRANS
		[trans1]: var1 => var1' = !var1
ENDMODULE

INSTANCE inst1 = mod1()
INSTANCE inst2 = mod2()


LTLSPEC G F just(inst2.trans1)
