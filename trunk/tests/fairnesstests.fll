OPTIONS
--FAULT_FAIR_DISABLE
--INST_WEAK_FAIR_DISABLE
CHECK_DEADLOCK
ENDOPTIONS

PROCTYPE proc()
	VAR
		var1:bool
	FAULT
		fault1: is TRANSIENT
	INIT
		var1 = TRUE
	TRANS
		[t1]:
ENDPROCTYPE

INSTANCE inst1 = proc()
INSTANCE inst2 = proc()

LTLSPEC G F just(inst1.t1)

LTLSPEC G F just(inst1.t1) & G F just(inst2.t1)

LTLSPEC G F just(inst1.t1) | G F just(inst2.t1)
